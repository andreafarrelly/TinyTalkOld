{
  "expo": {
    "name": "TinyTalk",
    "slug": "TinyTalk",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/tinytalkicon1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/tinytalkicon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffff0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.TinyTalk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "68a6ecd2-4b2f-4f4c-9462-f6bb598f7bb7"
      }
    }
  }
}
